set tabstop=2
set shiftwidth=2
set expandtab
set autoindent
set list
set number
syntax on
filetype on
filetype indent on
filetype plugin on
map <F5> <ESC>:tabnew<CR>
map <F6> <ESC>:tabclose<CR>
map <F7> <ESC>:tabp<CR>
map <F8> <ESC>:tabn<CR>
let g:SrcExpl_RefreshTime = 1
let g:SrcExpl_UpdateTags = 1
